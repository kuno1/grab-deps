{
	"name": "@kunoichi/grab-deps",
	"version": "3.0.10",
	"description": "Grab dependencies from js/css files.",
	"main": "index.js",
	"scripts": {
		"test": "mocha --ignore 'test/file-system-watch-test.js'",
		"test:full": "mocha",
		"test:fast": "mocha --ignore 'test/file-system-watch-test.js' --ignore 'test/wpscripts-test.js'",
		"test:ci": "mocha",
		"test:watch": "mocha --ignore 'test/file-system-watch-test.js' --watch",
		"lint": "wp-scripts lint-js '{index.js,lib/*.js,bin/grab-deps.js,bin/grab-images.mjs}'",
		"format": "eslint --fix --c ./node_modules/@wordpress/scripts/config/.eslintrc.js '{index.js,lib/*.js,bin/grab-deps.js,bin/grab-images.mjs}'",
		"imagemin": "npx grab-deps-image 'test/src/images/**/*.{jpg,png,gif,svg}' test/dist/images",
		"build:js": "npx grab-deps js test/src/js test/dist/js",
		"dump": "npx grab-deps dump 'test/src'",
		"dump:assets": "npx grab-deps dump 'test/assets/js'",
		"prepare": "husky"
	},
	"bin": {
		"grab-deps-image": "./bin/grab-images.mjs",
		"grab-deps": "./bin/grab-deps.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kuno1/grab-deps.git"
	},
	"keywords": [
		"wordpress"
	],
	"author": "Kunoichi Market",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kuno1/grab-deps/issues"
	},
	"homepage": "https://github.com/kuno1/grab-deps#readme",
	"dependencies": {
		"glob": "^11.0.1"
	},
	"devDependencies": {
		"@wordpress/scripts": "^27.9.0",
		"husky": "^9.1.7",
		"imagemin": "^9.0.0",
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-jpegtran": "^8.0.0",
		"imagemin-pngquant": "^10.0.0",
		"imagemin-svgo": "^11.0.1",
		"mocha": "^11.7.1"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"volta": {
		"node": "18.20.6"
	},
	"grabDeps": {
		"namespace": "testns",
		"js": {
			"srcDir": "test/src/js",
			"autoHandleGeneration": true,
			"globalExportGeneration": true
		},
		"css": {
			"srcDir": "test/src/css",
			"autoHandleGeneration": true
		}
	},
	"peerDependencies": {
		"@wordpress/scripts": ">=27.9.0",
		"imagemin": "^9.0.0",
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-jpegtran": "^8.0.0",
		"imagemin-pngquant": "^10.0.0",
		"imagemin-svgo": "^11.0.1"
	}
}
